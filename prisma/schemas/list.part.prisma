model ListRow {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @db.Timestamptz()
  content      String   @db.VarChar(255)
  list List @relation(fields: [listId], references: [id] )
  listId Int
}

model List {
  id Int @id @default(autoincrement())
  rows ListRow[]
  user User @relation(fields: [userId], references: [id])
  userId String
}

